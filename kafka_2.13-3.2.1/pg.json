{
  "name": "shop_main_connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "plugin.name": "pgoutput",
    "database.hostname": "std-base02.services.lab.x5.ru",
    "database.port": "5432",
    "database.user": "postgres",
    "database.password": "123qweasd",
    "database.dbname" : "shop_main",
    "database.server.name": "pg",
    "table.include.list": "order.order,order.order_info,order.position,order.payment",
    "heartbeat.interval.ms": "5000",
    "slot.name": "shop_main_debezium",
    "publication.name": "shop_main_publication",
    "snapshot.mode": "initial",
    "snapshot.locking.mode": "none",
    "decimal.handling.mode": "string",
    "interval.handling.mode": "string",
    "time.precision.mode": "connect",
    "key.converter.schemas.enable": "true",
    "key.converter.schemas.enable": "true",
    "internal.key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "internal.value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "transforms": "route,unwrap",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "pg.(.*).(.*)$",
    "transforms.route.replacement": "cdc.vprok.$1",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "key.converter.schemas.enable": "true",
    "value.converter.schemas.enable": "true"
  }
}
